    "apexcharts": "^3.23.1",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chai": "^4.3.4",
    "connect-redis": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "generate-password": "^1.6.0",
    "js-convert-case": "^4.1.2",
    "jsonwebtoken": "^8.5.1",
