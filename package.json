{
  "name": "capstone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "export NODE_ENV=test && mocha 'server/api/test/**/*.test.js' --recursive --timeout 60000 --exit",
